.table-cell
  boxSizing border-box
  overflow hidden
  text-overflow ellipsis
  white-space normal
  word-break break-all

.header-wrapper
  width 100%
  overflow hidden
  position relative

.body-wrapper
  width 100%
  overflow auto
  position relative

.table-row
  transition background-color .25s ease

  &:hover
    background-color #fcfcfc

.hidden
  display none
